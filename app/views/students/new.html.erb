<%= form_with model: @student do |form| %>
    <%= form.text_field :fname,placeholder:"first name" %>
    <%= form.text_field :lname ,placeholder:"last name"  %>
    <%= form.number_field :age ,placeholder:"age" %>
    <p>cohort id: <%= form.number_field :cohort_id %></p>
    <%= form.submit %>
  <% end %>