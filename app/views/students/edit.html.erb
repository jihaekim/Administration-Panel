<%= form_with model: @student do |form| %></p>
    <p>first name: <%= form.text_field :fname %></p>
    <p>last name: <%= form.text_field :lname %></p>
    <p>age: <%= form.number_field :age %></p>
    <%= form.text_field :pic_url,placeholder:"picture url" %>
    <p>cohort id: <%= form.number_field :cohort_id %></p>
    <%= form.submit %>
  <% end %>