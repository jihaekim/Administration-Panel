<%= form_with model: @teacher do |form| %>
    <%= form.text_field :fname, placeholder:"first name"%>
    <%= form.text_field :lname, placeholder:"last name" %>
    <%= form.text_field :age, placeholder:"age" %>
    <%= form.text_field :salary,placeholder:"salary"%>
    <%= form.text_field :pic_url,placeholder:"picture url" %>
    <%= form.text_field :education,placeholder:"education" %>
    <%= form.submit %>
<% end %>